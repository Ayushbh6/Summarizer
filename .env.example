# Environment Variables for AI Summarizer App
# Copy this file to .env.local for local development
# Set these variables in Railway dashboard for production

# Required: OpenRouter API Key for AI summarization
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Set to your Railway app URL for canonical URLs
NEXT_PUBLIC_APP_URL=https://your-app-name.up.railway.app

# Node Environment (automatically set in production)
NODE_ENV=development
